{
    // "window.autoDetectColorScheme": true,
    // "workbench.preferredLightColorTheme": "Monokai Dimmed",
    // "workbench.preferredDarkColorTheme": "Ayu Mirage",
    "workbench.colorTheme": "Monokai Dimmed",
    "editor.fontFamily": "'Monaspace Xenon', monospace",
    "editor.fontLigatures": "'calt', 'liga', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08', 'ss09'",
    "[javascript]": {
        "editor.tabSize": 2
    },
    "python.analysis.diagnosticSeverityOverrides": {
        "reportInvalidTypeForm": "none",
        "reportRedeclaration": "none",
    },
    "todo-tree.highlights.defaultHighlight": {
        "icon": "alert",
        "type": "text",
        "foreground": "yellow",
        "background": "gray",
        "opacity": 50,
        "iconColour": "yellow",
        "gutterIcon": true
    },
    "todo-tree.highlights.customHighlight": {
        "TEMP": {
            "icon": "issue-draft",
            "foreground": "white",
            "background": "gray",
            "iconColour": "white",
        },
        "TODO": {
            "icon": "check",
            "foreground": "white",
            "background": "gray",
            "iconColour": "white",
        },
        "NOTE": {
            "icon": "pin",
            "foreground": "black",
            "background": "gray",
            "iconColour": "black",
        },
        "REVIEW": {
            "icon": "eye",
            "foreground": "pink",
            "background": "gray",
            "iconColour": "pink",
        },
        "OPTIMIZE": {
            "icon": "zap",
            "foreground": "yellow",
            "background": "gray",
            "iconColour": "yellow",
        },
        "EXTEND": {
            "icon": "plug",
            "foreground": "cyan",
            "background": "gray",
            "iconColour": "cyan",
        },
        "FIXME": {
            "icon": "tools",
            "foreground": "orange",
            "background": "white",
            "iconColour": "orange",
        },
        "BUG": {
            "icon": "bug",
            "foreground": "red",
            "background": "white",
            "iconColour": "red",
        },
        "HACK": {
            "icon": "thumbsdown",
            "foreground": "purple",
            "background": "white",
            "iconColour": "purple",
        },
        "DEPRECATED": {
            "icon": "trash",
            "foreground": "gray",
            "background": "white",
            "iconColour": "gray",
        },
        "VIBE-CODED": {
            "icon": "hubot",
            "foreground": "lime",
            "background": "black",
            "iconColour": "lime",
        },
    }
}